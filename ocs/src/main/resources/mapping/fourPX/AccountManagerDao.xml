<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.it.ocs.fourPX.dao.AccountManagerDao">
	<select id="selectOneByCode" parameterType="java.lang.String" resultType="java.util.Map">
		select * from fpx_account_manager l where l.code=#{code}
	</select>
	
	<select id="selectDeOneByCode" parameterType="java.lang.String" resultType="java.util.Map">
		select * from fpx_account_manager_de l where l.code=#{code}
	</select>
</mapper>